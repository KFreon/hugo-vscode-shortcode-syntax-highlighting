{
    "name": "hugo-shortcode-syntax",
    "displayName": "Hugo Shortcode Syntax Highlighting",
    "description": "Enables syntax highlighting for Hugo shortcodes in markdown",
    "version": "1.0.0",
    "publisher": "kaellarkin",
    "repository": {"url": "https://github.com/kfreon/hugo-vscode-shortcode-syntax-highlighting.git", "type": "git"},
    "engines": {
        "vscode": "^1.41.1"
    },
    "keywords": [
        "Hugo", "Shortcode"
    ],
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "hugo",
            "aliases": ["hugo", "HUGO"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "hugo",
            "scopeName": "text.markdown.hugo",
            "path": "./syntaxes/hugo.tmLanguage.json",
            "injectTo": [
                "text.html.markdown"
            ]
        }]
    },
    "icon": "icon.png"
}